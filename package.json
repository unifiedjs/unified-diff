{
  "name": "unified-diff",
  "version": "3.0.0",
  "description": "unified plugin to ignore unrelated messages",
  "license": "MIT",
  "keywords": [
    "unified",
    "remark",
    "retext",
    "rehype",
    "plugin",
    "travis",
    "difference",
    "diff",
    "pr"
  ],
  "repository": "unifiedjs/unified-diff",
  "bugs": "https://github.com/unifiedjs/unified-diff/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "git-diff-tree": "^1.0.0",
    "vfile-find-up": "^5.0.0"
  },
  "devDependencies": {
    "execa": "^2.0.0",
    "nlcst-to-string": "^2.0.0",
    "nyc": "^14.0.0",
    "prettier": "^1.0.0",
    "remark-cli": "^6.0.0",
    "remark-preset-wooorm": "^5.0.0",
    "retext": "^6.0.0",
    "rimraf": "^2.5.4",
    "tape": "^4.0.0",
    "to-vfile": "^6.0.0",
    "trough": "^1.0.0",
    "unist-util-visit": "^1.1.0",
    "xo": "^0.24.0"
  },
  "scripts": {
    "format": "remark . -qfo && prettier --write \"**/*.js\" && xo --fix",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test/index.js",
    "test": "npm run format && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
